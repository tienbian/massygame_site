<section class="cta bg-primary">
  <div class="container">
      <h2 class="text-center" style="color: white;">TESTIMONIAL</h2>
      <div id="myCarousel" class="carousel slide" data-ride="carousel" style="min-height: 300px;">
        <!-- Indicators -->
        <ol class="carousel-indicators">
          <li data-target="#myCarousel" data-slide-to="0" class="active">
            <%= image_tag "home_page/customer_review_1.jpg", class:"img-responsive", alt:"" %>
          </li>
          <li data-target="#myCarousel" data-slide-to="1">
            <%= image_tag "home_page/customer_review_1.jpg", class:"img-responsive", alt:"" %>
          </li>
          <li data-target="#myCarousel" data-slide-to="2">
            <%= image_tag "home_page/customer_review_1.jpg", class:"img-responsive", alt:"" %>
          </li>
          <li data-target="#myCarousel" data-slide-to="3">
            <%= image_tag "home_page/customer_review_1.jpg", class:"img-responsive", alt:"" %>
          </li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner" role="listbox">
          <div class="item active">
            <%= image_tag "home_page/customer_review_1.jpg", class: "img-responsive", alt: "customer_review_1" %>
          </div>

          <div class="item">
            <%= image_tag "home_page/customer_review_1.jpg", class: "img-responsive", alt: "customer_review_1" %>
          </div>

          <div class="item">
            <%= image_tag "home_page/customer_review_1.jpg", class: "img-responsive", alt: "customer_review_1" %>
          </div>

          <div class="item">
            <%= image_tag "home_page/customer_review_1.jpg", class: "img-responsive", alt: "customer_review_1" %>
          </div>
        </div>

        <!-- Left and right controls -->
        <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
          <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
          <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
   </div>
</section>